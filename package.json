{
  "name": "emd-invoices-vite",
  "version": "1.0.0",
  "description": "React Redux full-stack RESTful application to manage invoices using Vite, React, Redux, Express, MongoDB, Cloudinary, AWS S3",
  "keywords": [
    "pwa",
    "template",
    "react",
    "redux",
    "redux-toolkit",
    "bootstrap",
    "heroku",
    "vite",
    "express",
    "api"
  ],
  "type": "module",
  "main": "backend/server.js",
  "scripts": {
    "build:prod": "webpack --config ./webpack.config.js --mode='production'",
    "build:dev": "webpack",
    "test": "jest --passWithNoTests --updateSnapshot --coverage",
    "client": "npm start --prefix frontend",
    "start:dev": "webpack && node dist/server.cjs",
    "server": "node dist/server.cjs",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint backend/**/*.js",
    "heroku-prebuild": "NPM_CONFIG_PRODUCTION=false npm install && npm run build:prod",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend",
    "cancel": "heroku builds:cancel -a emd-invoices-vite",
    "logs": "heroku logs -a emd-invoices-vite --tail",
    "purge": "heroku builds:cache:purge -a emd-invoices-vite",
    "stop": "heroku ps:stop -a emd-invoices-vite"
  },
  "author": "Stephen Vaughan",
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.367.0",
    "axios": "^1.8.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "cloudinary": "^1.34.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.21.1",
    "express-async-handler": "^1.2.0",
    "express-fileupload": "^1.4.0",
    "express-rate-limit": "^6.7.0",
    "generate-password": "^1.7.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.1.0",
    "mongoose": "^8.8.3",
    "nodemailer": "^6.9.9",
    "pretty-error": "^4.0.0",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.0.1",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.40",
    "@swc/jest": "^0.2.24",
    "concurrently": "^7.6.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-html-reporter": "^3.7.0",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "webpack-dev-server": "^5.2.1"
  }
}
